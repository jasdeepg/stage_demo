<h1>Listing energy_data</h1>

<table>
  <tr>
    <th>Month</th>
    <th>Day</th>
    <th>Year</th>
    <th>Hour</th>
    <th>Power</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @energy_data.each do |energy_datum| %>
  <tr>
    <td><%= energy_datum.month %></td>
    <td><%= energy_datum.day %></td>
    <td><%= energy_datum.year %></td>
    <td><%= energy_datum.hour %></td>
    <td><%= energy_datum.power %></td>
    <td><%= energy_datum.user_id %></td>
    <td><%= link_to 'Show', energy_datum %></td>
    <td><%= link_to 'Edit', edit_energy_datum_path(energy_datum) %></td>
    <td><%= link_to 'Destroy', energy_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

